<root BTCPP_format="4">
    <BehaviorTree ID="GripperControl">
        <Sequence>
            <!-- Close the gripper -->
            <OpenCloseGripper command="close" speed="10"/>
            
            <!-- Wait 2 seconds before opening -->
            <Wait delay_msec="10000"/>
            
            <!-- Open the gripper -->
            <OpenCloseGripper command="open" speed="10"/>
        </Sequence>
    </BehaviorTree>
</root>
