<root BTCPP_format="4">
  <BehaviorTree ID="GripperControl">
      <Sequence>
        <OpenCloseGripper command="open" speed="10"/>
        <TagPose id="0" success="{tag1_success}" x="{tag1_x}" y="{tag1_y}" z="{tag1_z}"/>
        <Script code="adjusted_x := tag1_x + 0.04"/>
        <Script code="adjusted_y := tag1_y + 0.04"/>
        <Script code="adjusted_z := -tag1_z + 0.045"/>
        <Precondition if="tag1_success">
          <MoveTo x="{adjusted_x}" y="{adjusted_y}" z="{adjusted_z}" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
        </Precondition>
        <OpenCloseGripper command="close" speed="10"/>
        <MoveToPose x="0.15" y="-0.047" z="0.295" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
        <OpenCloseGripper command="open" speed="10"/>
      </Sequence>
  </BehaviorTree>
</root>