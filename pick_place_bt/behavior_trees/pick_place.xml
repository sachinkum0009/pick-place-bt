<root BTCPP_format="4">
    <BehaviorTree ID="GripperControl">
        <Repeat num_cycles="10">
            <Fallback>
                <Sequence>
                    <MoveTo x="0.25" y="-0.11" z="0.165" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
                    <MoveTo x="0.25" y="-0.11" z="0.065" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
                    <Wait delay_msec="2000"/>
                    <OpenCloseGripper command="close" speed="10"/>
                    <Wait delay_msec="2000"/>
                    <MoveTo x="0.15" y="0.0" z="0.25" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
                    <Wait delay_msec="2000"/>
                    <MoveTo x="0.25" y="0.11" z="0.165" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
                    <MoveTo x="0.25" y="0.11" z="0.065" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
                    <Wait delay_msec="2000"/>
                    <OpenCloseGripper command="open" speed="10"/>
                </Sequence>

                <!-- fallback: if the Sequence fails, go to this MoveTo -->
                <MoveTo x="0.15" y="-0.11" z="0.25" wx="-0.0503060" wy="0.998189" wz="-0.005559" w="-0.032425"/>
            </Fallback>
        </Repeat>
    </BehaviorTree>
</root>
